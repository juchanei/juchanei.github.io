{
  "name": "juchanei.github.io",
  "description": "Personal blog for Software development",
  "version": "1.0.0",
  "private": true,
  "author": "LEE Juchan <juchanei.lee@gmail.com>",
  "keywords": [
    "Blog",
    "Software development",
    "Programming"
  ],
  "scripts": {
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "deploy": "npm run build && gh-pages -d public"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "@gatsbyjs/reach-router": "^1.3.6",
    "babel-plugin-styled-components": "^1.12.0",
    "gatsby": "^3.10.2",
    "gatsby-image": "^3.10.0",
    "gatsby-plugin-react-helmet": "^4.5.0",
    "gatsby-plugin-sharp": "^3.10.2",
    "gatsby-plugin-styled-components": "^4.10.0",
    "gatsby-remark-images": "^5.7.0",
    "gatsby-remark-relative-images": "^2.0.2",
    "gatsby-source-filesystem": "^3.10.0",
    "gatsby-transformer-remark": "^3.2.0",
    "gatsby-transformer-sharp": "^3.10.0",
    "postcss": "^8.3.6",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-helmet": "^6.1.0",
    "styled-components": "^5.2.1",
    "tsconfig-paths-webpack-plugin": "3.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@types/node": "14.14.20",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-helmet": "^6.1.1",
    "@types/styled-components": "^5.1.11",
    "@typescript-eslint/eslint-plugin": "4.13.0",
    "@typescript-eslint/parser": "4.13.0",
    "babel-plugin-root-import": "6.6.0",
    "commitizen": "4.2.2",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.17.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "7.1.0",
    "eslint-import-resolver-root-import": "1.0.4",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "gh-pages": "^3.1.0",
    "lint-staged": "10.5.3",
    "prettier": "2.2.1",
    "typescript": "4.1.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
